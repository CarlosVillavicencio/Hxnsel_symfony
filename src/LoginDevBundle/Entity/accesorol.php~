<?php

namespace LoginDevBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * accesorol
 *
 * @ORM\Table(name="accesorol")
 * @ORM\Entity(repositoryClass="LoginDevBundle\Repository\accesorolRepository")
 */
class accesorol {

    /**
     * @var int
     *
     * @ORM\Column(name="idrol", type="integer")
     * @ORM\Id
     */
    private $idrol;

    /**
     * @var string
     *
     * @ORM\Column(name="rolvnombre", type="string", length=70, unique=true)
     */
    private $rolvnombre;

    /**
     * @var string
     *
     * @ORM\Column(name="rolvdescrip", type="string", length=4000, nullable=true)
     */
    private $rolvdescrip;

    /**
     * @var string
     *
     * @ORM\Column(name="rolcestado", type="string", length=1)
     */
    private $rolcestado;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="roldfechaaudit", type="datetime")
     */
    private $roldfechaaudit;

    /**
     * @var string
     *
     * @ORM\Column(name="rolvusuarioaudit", type="string", length=32)
     */
    private $rolvusuarioaudit;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="roldfechaauditult", type="datetime")
     */
    private $roldfechaauditult;

    /**
     * @var string
     *
     * @ORM\Column(name="rolvusuarioauditult", type="string", length=32)
     */
    private $rolvusuarioauditult;

}
